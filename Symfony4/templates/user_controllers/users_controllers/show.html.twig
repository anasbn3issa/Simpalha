<!DOCTYPE html>

<title>Users</title>

{% block body %}
    <h1>Users</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ user.email }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ user.password }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ user.username }}</td>
            </tr>
            <tr>
                <th>Specialite</th>
                <td>{{ user.specialite }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ user.code }}</td>
            </tr>
            <tr>
                <th>About</th>
                <td>{{ user.about }}</td>
            </tr>
            <tr>
                <th>Role</th>
                <td>{{ user.role }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('user_controllers_users_controllers_index') }}">back to list</a>

    <a href="{{ path('user_controllers_users_controllers_edit', {'id': user.id}) }}">edit</a>

    {{ include('user_controllers_users_controllers/_delete_form.html.twig') }}
{% endblock %}
