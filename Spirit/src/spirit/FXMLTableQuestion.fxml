<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spirit.FXMLTableQuestionController">
   <children>
      <TableView fx:id="LAffiche" layoutY="-3.0" prefHeight="323.0" prefWidth="661.0">
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="50.0" text="ID">
               <cellValueFactory>
                  <PropertyValueFactory property="id" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="questionColumn" prefWidth="483.0" text="Question">
               <cellValueFactory>
                  <PropertyValueFactory property="question" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="answerColumn" prefWidth="123.0" text="Answer">
               <cellValueFactory>
                  <PropertyValueFactory property="answer" />
               </cellValueFactory>
            </TableColumn>
         </columns>
      </TableView>
      <Button layoutX="60.0" layoutY="361.0" mnemonicParsing="false" onAction="#addQuestion" prefHeight="25.0" prefWidth="168.0" text="Add" />
      <Button layoutX="243.0" layoutY="361.0" mnemonicParsing="false" onAction="#editQuestion" prefHeight="25.0" prefWidth="168.0" text="Edit" />
      <Button layoutX="434.0" layoutY="361.0" mnemonicParsing="false" onAction="#deleteQuestion" prefHeight="25.0" prefWidth="168.0" text="Delete" />
      <Button layoutX="290.0" layoutY="328.0" mnemonicParsing="false" onAction="#reloadQuestions" prefHeight="25.0" prefWidth="81.0" text="Reload" />
      <Button layoutX="59.0" layoutY="328.0" mnemonicParsing="false" onAction="#addAnswers" prefHeight="25.0" prefWidth="168.0" text="Add Answers" />
      <Button layoutX="434.0" layoutY="328.0" mnemonicParsing="false" onAction="#showAnswers" prefHeight="25.0" prefWidth="168.0" text="Show Answers" />
   </children>
</AnchorPane>
