<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="451.0" prefWidth="661.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spirit.FXMLQuestionTableController">
   <children>
      <TableView fx:id="LAffiche" layoutY="-3.0" prefHeight="365.0" prefWidth="661.0">
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="50.0" text="ID">
               <cellValueFactory>
                  <PropertyValueFactory property="id" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="questionColumn" prefWidth="483.0" text="Question">
               <cellValueFactory>
                  <PropertyValueFactory property="question" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="answerColumn" prefWidth="123.0" text="Answer">
               <cellValueFactory>
                  <PropertyValueFactory property="answer" />
               </cellValueFactory>
            </TableColumn>
         </columns>
      </TableView>
      <Button layoutX="60.0" layoutY="373.0" mnemonicParsing="false" onAction="#addQuestion" prefHeight="25.0" prefWidth="168.0" text="Add a Question" />
      <Button layoutX="243.0" layoutY="373.0" mnemonicParsing="false" onAction="#editQuestion" prefHeight="25.0" prefWidth="168.0" text="Edit a Question" />
      <Button layoutX="434.0" layoutY="373.0" mnemonicParsing="false" onAction="#deleteQuestion" prefHeight="25.0" prefWidth="168.0" text="Delete a Question" />
      <Button fx:id="addQuizzButton" layoutX="257.0" layoutY="412.0" mnemonicParsing="false" onAction="#addQuizz" prefHeight="25.0" prefWidth="141.0" text="Confirm Quizz" />
   </children>
</AnchorPane>
